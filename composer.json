{
    "name": "sirna/patrickowen",
    "description": "project package for patrickowen site",
    "license": "proprietary",
    "homepage": "http://patrickowen.dev",
    "authors": [
        {
            "name": "Sirna",
            "email": "f@kema.il",
            "homepage": "http://sirna.id",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "wordpress",
                "type": "webroot",
                "version": "4.1.1",
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/WordPress/WordPress/archive/4.1.1.zip"
                },
                "require": {
                    "fancyguy/webroot-installer": "1.1.0"
                }
            }
        },
        {
            "type": "composer",
            "url": "http://wpackagist.org"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "wordpress": "4.1.1",
        "wpackagist-plugin/admin-menu-editor": "1.4.3",
        "wpackagist-plugin/visual-form-builder": "2.8.1",
        "fancyguy/webroot-installer": "1.1.0",
        "wpackagist-plugin/woocommerce": "2.3.5",
        "wpackagist-plugin/woocommerce-cart-tab": "0.3.1",
        "wpackagist-plugin/woocommerce-customizer": "2.1.0",
        "wpackagist-plugin/woocommerce-attributes-menu-manager": "0.2",
        "wpackagist-plugin/newsletter": "3.6.7",
        "wpackagist-plugin/admin-toolbar-menus": "1.0.1",
        "wpackagist-plugin/revision-control": "2.3.1",
        "wpackagist-plugin/woocommerce-product-archive-customiser": "0.5.0",
        "wpackagist-plugin/woocommerce-product-details-customiser": "0.2.0",
        "wpackagist-plugin/woocommerce-product-sku-generator": "1.2.1",
        "wpackagist-plugin/yith-woocommerce-zoom-magnifier": "1.1.5",
        "wpackagist-plugin/woocommerce-product-image-flipper": "0.2.0",
        "wpackagist-plugin/regenerate-thumbnails": "2.2.4",
        "wpackagist-plugin/wysija-newsletters": "2.6.15",
        "wpackagist-plugin/soliloquy-lite": "2.2.2"
    },
    "config": {
        "bin-dir": "bin/",
        "process-timeout": 1000
    },
    "extra": {
        "webroot-dir": "site",
        "webroot-package": "wordpress",
        "installer-paths": {
            "content/plugins/{$name}/": ["type:wordpress-plugin"],
            "content/themes/{$name}/": ["type:wordpress-theme"]
        }
    },
    "require-dev": {
        "wpackagist-plugin/developer": "1.2.5",
        "wpackagist-plugin/debug-bar": "0.8.2",
        "wpackagist-plugin/query-monitor": "2.6.10",
        "wpackagist-plugin/theme-check": "20141222.1",
        "wpackagist-plugin/unyson": "2.1.25",
        "wpackagist-plugin/wordpress-database-reset": "2.3.1",
        "wpackagist-plugin/wp-optimize": "1.8.9.7",
        "wpackagist-plugin/hide-admin-bar": "0.2.6"
    }
}
